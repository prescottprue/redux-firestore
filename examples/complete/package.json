{
  "name": "complete",
  "version": "0.0.1",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:dist": "npm run build && firebase emulators:start --only hosting",
    "start:emulate": "cross-env REACT_APP_FIREBASE_DATABASE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.database.port)\" REACT_APP_FIRESTORE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.firestore.port)\" npm run start",
    "emulators": "firebase emulators:start --only firestore,database,pubsub",
    "emulators:hosting": "firebase emulators:start --only firestore,database,hosting",
    "emulators:all": "firebase emulators:start",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/complete.git"
  },
  "engines": {
    "node": ">12.18.0"
  },
  "author": "prescottprue (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "firebase": "^8.2.6",
    "notistack": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-button": "^0.7.2",
    "react-redux": "^7.1.1",
    "react-redux-firebase": "^3.8.1",
    "react-hook-form": "^6.15.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-auth-wrapper": "^2.1.0",
    "redux-firestore": "^0.11.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@sentry/cli": "1.62.0",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "5.0.0",
    "firebase-ci": "0.15.1",
    "firebase-tools": "9.3.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react-scripts": "4.0.2"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts}": [
      "eslint --fix"
    ]
  },
  "config": {
    "port": 3000
  }
}
